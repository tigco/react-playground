{
  "name": "react-playground",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/tigco/react-playground.git"
  },
  "description": "React development environment with webpack, babel, eslint, and mocha.",
  "main": "index.jsx",
  "scripts": {
    "start": "webpack-dev-server --config ./webpack.config.js --mode development",
    "test": "npm run test:unit",
    "test:unit": "mocha --require babel-core/register --require ./test/helpers.js --require ./test/dom.js --require ignore-styles 'src/**/*.test.js'",
    "test:unit:watch": "npm run test:unit -- --watch"
  },
  "keywords": [],
  "author": "Tigran Mirzoev",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.22.0",
    "chai": "^4.2.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "ignore-styles": "^5.0.1",
    "jsdom": "^11.12.0",
    "mocha": "^5.2.0",
    "react-hot-loader": "^3.1.3",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "dependencies": {
    "babel-preset-env": "^1.7.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3"
  }
}
